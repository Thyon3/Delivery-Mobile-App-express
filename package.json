{
  "name": "delivery-api",
  "version": "1.1.0",
  "description": "Production-ready Express.js REST API for Multi-vendor Delivery Application supporting Customers, Drivers, and Restaurants with real-time tracking, geolocation services, and payment integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:workers": "nodemon --exec ts-node src/workers/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:workers": "node dist/workers/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node scripts/seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "delivery",
    "express",
    "typescript",
    "postgresql",
    "postgis",
    "socket.io",
    "rest-api",
    "backend",
    "nodejs",
    "prisma",
    "redis",
    "stripe",
    "real-time",
    "geolocation",
    "multi-vendor"
  ],
  "author": "Backend Architect",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "express": "^4.18.2",
    "typescript": "^5.3.3",
    "socket.io": "^4.6.1",
    "redis": "^4.6.12",
    "bullmq": "^5.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "argon2": "^0.31.2",
    "zod": "^3.22.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "stripe": "^14.10.0",
    "dotenv": "^16.3.1",
    "express-async-errors": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.1.1",
    "graphql-scalars": "^1.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/morgan": "^1.9.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^30.2.0",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/delivery-api.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/delivery-api/issues"
  },
  "homepage": "https://github.com/yourusername/delivery-api#readme"
}
